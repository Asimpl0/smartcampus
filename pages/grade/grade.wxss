Page {
  background-color: #f4f4f4;
  
}
.page{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-self: flex-start;
  align-items: center;
}
.tabs{
  width:100%;
  margin-bottom: 15rpx;
}
.title{
  height: 50rpx;
  text-align: center;
}

.score{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  border-radius: 15rpx;
  width: 90%;
  height: 180rpx;
  margin-bottom: 15rpx;
}
.detail{
  display: flex;
  height: 90%;
  flex-direction: column;
  justify-content: space-around;
  align-items: flex-start;
  margin-left: 40rpx;
  }
.grade{
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  margin-right: 40rpx;
  align-items: center;
}


